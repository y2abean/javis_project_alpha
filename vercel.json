{
    "buildCommand": "cd vite-react-app && npm install && npm run build",
    "outputDirectory": "vite-react-app/dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/chat",
            "destination": "/api/chat"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}