{
    "version": 2,
    "builds": [
        {
            "src": "vite-react-app/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/vite-react-app/dist/$1"
        }
    ]
}